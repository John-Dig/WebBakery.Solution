@{
  Layout = "_Layout";
}

@using Bakery.Models;

<h3>Here are all the orders for this vendor</h3>

<ol>
Model: System.Collections.Generic.Dictionary`2[System.String,System.Object];
  @foreach (Order order in @Model["orders"])
  {
    <li><a href='vendors/@Model["vendor"].Id/orders/@order.Id'>@order.Title</a></li>
  } 
</ol>
<p><a href='/vendors/@Model["vendor"].Id/orders/new'></a></p>
<p><a href='/vendors'>Return to vendors list</a></p>
<p><a href='/'>Return to Main Page</a></p>


